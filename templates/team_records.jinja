<tr class="mat-header-row" role="row">
    <td class="mat-header-cell" role="columnheader"></td>
    <td class="mat-header-cell" role="columnheader"></td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.points }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.field_goals_made }}-{{ team_records.field_goals_attempted }}</td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.three_point_field_goals_made }}-{{ team_records.three_point_field_goals_attempted }}</td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.free_throws_made }}-{{ team_records.free_throws_attempted }}</td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.rebounds_offensive }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.rebounds_defensive }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.rebounds_total }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.assists }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.steals }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.blocked_shots }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.turnovers }} </td>
    <td class="mat-header-cell" role="columnheader">{{ team_records.personal_fouls }} </td>
    <td></td>
</tr>
<tr class="mat-header-row" role="row">
<td class="mat-header-cell" role="columnheader"></td>
<td class="mat-header-cell" role="columnheader"></td>
<td class="mat-header-cell" role="columnheader"></td>
<td class="mat-header-cell" role="columnheader">{{ team_records.field_goals_percentage }}</td>
<td class="mat-header-cell" role="columnheader">{{ team_records.three_point_field_goals_percentage }}</td>
<td class="mat-header-cell" role="columnheader">{{ team_records.free_throws_percentage }}</td>
</tr>